{
    "type": "postgres",
    "host": "${DATABASE_HOST}",
    "port": "${DATABASE_PORT}",
    "username": "${DATABASE_USER}",
    "password": "${DATABASE_PASSWORD}",
    "database": "${DATABASE_NAME}",
    "synchronize": true,
    "logging": true,
    "entities": ["dist/**/*.entity.js"],
    "migrations": ["dist/migrations/*.js"],
    "cli": {
      "entitiesDir": "src/entities",
      "migrationsDir": "src/migrations"
    }
  }
  